<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcfc.spring_demo.mapper.UserMapper">
    <!-- 数据库字段名和实体属性一致 -->
    <select id="selectOne" resultType="com.lcfc.spring_demo.entity.User" parameterType="String">
        select * from user where user.empno= #{empno}
    </select>
</mapper>